{
  "name": "ip2ldb-reader",
  "version": "2.1.6",
  "description": "Reader for IP2Location databases",
  "main": "lib/index.js",
  "scripts": {
    "build": "npm run lint && npm run clean && npm run compile",
    "build-release": "npm run lint && npm run clean && npm run compile-release",
    "clean": "rimraf lib",
    "cli-testing": "node lib/cli-testing.js",
    "compile": "tsc --build tsconfig.json",
    "compile-release": "tsc --build tsconfig.release.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint src",
    "prepare": "npm run clean && npm run compile-release",
    "test": "jest"
  },
  "files": [
    "/lib"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mdmower/ip2ldb-reader.git"
  },
  "keywords": [
    "ip2location",
    "geolocation"
  ],
  "author": "Matt Mower <mdmower@cmphys.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mdmower/ip2ldb-reader/issues"
  },
  "homepage": "https://github.com/mdmower/ip2ldb-reader#readme",
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "verbose": true,
    "modulePathIgnorePatterns": [
      "<rootDir>/lib/"
    ]
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^18.19.33",
    "@typescript-eslint/eslint-plugin": "^7.9.0",
    "@typescript-eslint/parser": "^7.9.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jsdoc": "^48.2.5",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.7",
    "ts-jest": "^29.1.2",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "csv-parser": "^3.0.0"
  }
}
